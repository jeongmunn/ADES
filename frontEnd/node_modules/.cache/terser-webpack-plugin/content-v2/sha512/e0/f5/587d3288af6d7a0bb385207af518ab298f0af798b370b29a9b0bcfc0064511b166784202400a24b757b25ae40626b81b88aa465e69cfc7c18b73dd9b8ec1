oud\",onClick:function(){return e.isShowPopup(!0,6)},children:Object(I.jsx)(\"img\",{id:\"cloud6img\",alt:\"\",src:Ce})}),Object(I.jsx)(\"div\",{id:\"cloud7\",className:\"cloud\",onClick:function(){return e.isShowPopup(!0,7)},children:Object(I.jsx)(\"img\",{id:\"cloud7img\",alt:\"\",src:Pe})}),Object(I.jsx)(\"div\",{id:\"cloud8\",className:\"cloud\",onClick:function(){return e.isShowPopup(!0,8)},children:Object(I.jsx)(\"img\",{id:\"cloud8img\",alt:\"\",src:De})}),Object(I.jsx)(\"div\",{id:\"animate\",children:Object(I.jsx)(\"img\",{id:\"imgAnimate\",className:\"bounce\",alt:\"\",src:ye})}),Object(I.jsxs)(\"div\",{id:\"levelDisplay\",children:[Object(I.jsx)(\"p\",{id:\"level\",children:this.state.maze}),Object(I.jsx)(\"p\",{id:\"staticLevel\",children:\"Level\"})]}),Object(I.jsx)(\"div\",{className:\"icon\",children:Object(I.jsx)(c.b,{to:\"/studentDashboard\",className:\"navlink nav-link-ltr\",children:Object(I.jsx)(Le.a,{icon:Be.a})})})]}),Object(I.jsx)(Ee,{showModalPopup:this.state.showModalPopup,onPopupClose:this.isShowPopup,level:this.state.level,point:this.state.points,onNewLevel:this.handleNewLevel,id:this.state.id})]})}}]),a}(n.a.Component));a(240),a(241);var Te=function(){var e=Object(s.useState)({}),t=Object(r.a)(e,2),a=(t[0],t[1]);return Object(b.c)(x,(function(e){try{a(e)}catch(n){var t=n.code,s=n.message;console.log(t),console.log(s)}})),Object(I.jsxs)(I.Fragment,{children:[Object(I.jsx)(se.a,{}),Object(I.jsxs)(l.c,{children:[Object(I.jsx)(l.a,{path:\"/\",element:Object(I.jsx)(D,{})}),Object(I.jsx)(l.a,{path:\"/studentDashboard\",element:Object(I.jsx)(X,{})}),Object(I.jsx)(l.a,{path:\"/studentPoints\",element:Object(I.jsx)(te,{})}),Object(I.jsx)(l.a,{path:\"/studentBadges\",element:Object(I.jsx)(ne,{})}),Object(I.jsx)(l.a,{path:\"/studentReward\",element:Object(I.jsx)(ce,{})}),Object(I.jsx)(l.a,{path:\"/profile\",element:Object(I.jsx)(oe,{})}),Object(I.jsx)(l.a,{path:\"/teacherDashboard\",element:Object(I.jsx)(ie,{})}),Object(I.jsx)(l.a,{path:\"/leaderboa