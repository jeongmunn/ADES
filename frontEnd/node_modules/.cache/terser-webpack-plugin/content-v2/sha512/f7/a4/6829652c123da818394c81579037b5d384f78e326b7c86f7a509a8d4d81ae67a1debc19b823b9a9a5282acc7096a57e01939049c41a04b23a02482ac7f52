' }} key={item.studentID}>\n                                        <ListGroup variant=\"flush\" >\n                                            <ListGroup.Item>\n                                                <Row className=\"leaderBoardItem\">\n                                                    <Col>\n                                                        {item.name}\n                                                    </Col>\n                                                    <Col>\n                                                        {item.totalPts}\n                                                    </Col>\n                                                </Row>\n                                            </ListGroup.Item>\n                                        </ListGroup>\n                                    </Card>\n                                )}\n                            </Card.Body>\n                        </Card>\n                    </Col>\n                </Row>\n            </Container>\n        )\n    }\n}\n\nStudentDashboard.propTypes = {\n    classes: PropTypes.object.isRequired,\n};\n\nexport default withStyles(styles)(StudentDashboard);","import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport { Link } from 'react-router-dom';\nimport { Navbar, Container, Nav, Button } from 'react-bootstrap';\nimport '../css/navigation.css';\nimport sample from '../logoLoading.webm';\n\n\nexport default class StudentNavigation extends React.Component {\n\n\n    render() {\n\n        return (\n            <Navbar collapseOnSelect expand='sm' bg=\"light\" variant=\"light\" className=\"shadow p-3 mb-5 bg-white rounded\">\n                <Container>\n                    <Navbar.Toggle aria-controls='resoponsive-navbar-nav' />\n                    <Navbar.Collapse id=\"responsive-navbar-nav\">\n                        <Navbar.Brand href=\"#home\">\n                            <video className='videoTag' autoPlay loop mu