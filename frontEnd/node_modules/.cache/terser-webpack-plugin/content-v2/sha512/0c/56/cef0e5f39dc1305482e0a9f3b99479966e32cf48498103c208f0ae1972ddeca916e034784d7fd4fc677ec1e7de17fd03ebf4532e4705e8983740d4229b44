ie the data that the card needs for its creation\n                    var cardInfo = {\n                        \"quizID\": quiz.quizID,\n                        \"totalMarks\": quiz.totalMarks,\n                        \"totalPoints\": quiz.totalPoints\n\n                    }\n                    console.log(\" -----------Card Info data pack-----------\");\n                    console.log(cardInfo);\n\n\n\n                    var newCard = createCard(cardInfo);\n                    $('#users').append(newCard);\n\n                }\n\n\n            });\n\n\n        function createCard(cardInfo) {\n            console.log(cardInfo)\n            var card = `\n        <div class=\"card\" style=\" width:60% ;margin-right:10px;\">\n        <div class=\"card-body\">\n           \n           \n        </div> <p class=\"card-text\" style=\" font-size:20px;margin-left:20px\">Quiz ${cardInfo.quizID}</p>\n        <div style=\"margin-left:20px \"> \n        <div >\n            Total Marks:${cardInfo.totalMarks}\n        </div>\n        <div >\n            Total Points:${cardInfo.totalPoints}\n        </div>\n        <Link to={\"/EditBadge?id=${cardInfo.quizID}\"}>\n                      <Button>Do quiz</Button>\n                    </Link>\n        \n          </div>\n        \n         </div>\n            `;\n\n            return card;\n        }\n\n\n\n\n    }\n\n    //NEED TO GET STUDENT ID!\n\n\n\n\n\n\n\n    render() {\n\n        const data = this.state.data;\n\n        return (\n\n            <div>\n                <div id=\"users\" class=\"row\">\n\n\n                </div>\n\n\n            </div>\n        )\n    }\n}","export default __webpack_public_path__ + \"static/media/wisp.3ef4ee3f.png\";","export default __webpack_public_path__ + \"static/media/cloud1.1e2e981d.png\";","export default __webpack_public_path__ + \"static/media/cloud2.ecff1f7e.png\";","export default __webpack_public_path__ + \"static/media/cloud3.b4065f4e.png\";","export default __webpack_public_pa