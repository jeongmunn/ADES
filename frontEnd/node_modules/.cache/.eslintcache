}\n    render() {\n        return (\n            <div className=\"uploadReward\">\n                <h1>Upload Reward</h1>\n                <form onSubmit={this.handleSubmit}>\n                    <label>\n                        Reward Name :\n            <input type=\"text\" name=\"rewardName\" onChange={this.handleName} />\n                    </label>\n                    <label>\n                        Points Required :\n            <input type=\"number\" name=\"ptsRequired\" onChange={this.handlePoints} />\n                    </label>\n                    <label>\n                        Picture URL :\n            <input type=\"file\" name=\"myImage\" onChange={this.handleURL} />\n                    </label>\n                    <button type=\"submit\">Add</button>\n                </form>\n            </div>\n        )\n    }\n}","/Users/isaactan17/Desktop/ADES/frontEnd/src/components/BadgeAdmin.js",[],"/Users/isaactan17/Desktop/ADES/frontEnd/src/components/EditMazeContent.js",[],"/Users/isaactan17/Desktop/ADES/frontEnd/src/components/MazeAdmin.js",[],"/Users/isaactan17/Desktop/ADES/frontEnd/src/components/EditBadge.js",[],"/Users/isaactan17/Desktop/ADES/frontEnd/src/components/StudentDashboard.js",["73","74","75","76","77","78"],{"ruleId":"79","severity":1,"message":"80","line":3,"column":8,"nodeType":"81","messageId":"82","endLine":3,"endColumn":12},{"ruleId":"79","severity":1,"message":"83","line":20,"column":10,"nodeType":"81","messageId":"82","endLine":20,"endColumn":14},{"ruleId":"84","severity":1,"message":"85","line":75,"column":39,"nodeType":"86","messageId":"87","endLine":75,"endColumn":41},{"ruleId":"84","severity":1,"message":"85","line":77,"column":46,"nodeType":"86","messageId":"87","endLine":77,"endColumn":48},{"ruleId":"79","severity":1,"message":"83","line":93,"column":19,"nodeType":"81","messageId":"82","endLine":93,"endColumn":23},{"ruleId":"79","severity":1,"message":"88","line":7,"column":5,"nodeType":"81","messageId":"82","endLine":7,"